{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/api/*", "*.css", "*.js", "*.png", "*.jpg", "*.jpeg", "*.gif"]
  },
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self' 'unsafe-inline' https: data: blob:",
    "X-Frame-Options": "DENY"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "mimeTypes": {
    ".json": "application/json",
    ".css": "text/css",
    ".js": "application/javascript",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".gif": "image/gif"
  }
}
